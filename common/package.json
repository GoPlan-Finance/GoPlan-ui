{
  "name": "goplan-common",
  "version": "0.0.0",
  "description": "a financial planning platform for the general public.",
  "scripts": {
    "pre-commit": "npm run lint-fix && cd ../backend && npm run lint-fix && cd ../frontend && npm run lint-fix",
    "npm-update": "npm update && cd ../backend && npm update && cd ../frontend && npm update",
    "build": "cd ../backend && npm i && npm run build && cd ../frontend &&  npm i && npm run build",
    "lint": "eslint --cache .  --ext ts --ext js ",
    "lint-fix": "eslint --cache --fix .  --ext ts --ext js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoPlan-Finance/GoPlan-app.git"
  },
  "keywords": [
    "finance",
    "plan",
    "stocks",
    "stock-market",
    "dashboard"
  ],
  "author": "GoPlan-Finance team",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GoPlan-Finance/GoPlan-app/issues"
  },
  "homepage": "https://github.com/GoPlan-Finance/GoPlan-app#readme",
  "dependencies": {
    "crypto-js": "^4.0.0",
    "papaparse": "^5.3.0",
    "ts-money": "^0.4.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/node": "^14.14.37",
    "@types/parse": "^2.18.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-plugin-varspacing": "^1.2.2",
    "eslint-plugin-vue": "^7.9.0",
    "typescript": "^4.2.4"
  }
}
